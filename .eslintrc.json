{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile": false
    },
    "ignorePatterns": ["/node_modules/", "webpack.config.js"],
    "rules": {
        "indent": "off",
        "max-classes-per-file": "off"
    },
    "parser": "@babel/eslint-parser"
}